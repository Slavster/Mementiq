{
  "extends": "./tsconfig.json",
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "**/*.test.ts",
    "server/vite.ts",
    "shared/schema.ts"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strict": false,
    "allowJs": true,
    "checkJs": false,
    "noEmit": false,
    "outDir": "./dist",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "bundler"
  }
}